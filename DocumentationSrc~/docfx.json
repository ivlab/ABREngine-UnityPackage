{
  "metadata": [
    {
      "src": [
        {
          "src": "../../../",
          "files": [
            "*.csproj"
          ]
        }
      ],
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "dest": "api"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appFooter": "<a href=\"https://ivlab.cs.umn.edu\">Interactive Visualization Lab</a><br>Copyright 2023, Regents of the University of Minnesota, All Rights Reserved",
      "_appLogoPath": "resources/logo.svg",
      "_appFaviconPath": "resources/favicon.ico",
      "_enableSearch": true,

      "_appTitle": "ABREngine",
      "_gitUrlPattern": "github",
      "newFileRepository": {
        "branch": "master"
      },
      "_gitContribute": {
        "repo": "https://github.com/ivlab/ABREngine-UnityPackage",
        "branch": "master",
        "apiSpecFolder": "Documentation~/apidocs-overrides"
      }
    },
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md",
          "api/**.yml",
          "api/**.md",
          "manual/**.yml",
          "manual/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/**",
          "manual/resources/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidocs-edits/*.md"
        ]
      }
    ],
    "xref": [
      "resources/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "template": "templates/statictoc-with-github-umn-edu",
    "dest": "../Documentation~"
  }
}