{
    "name": "edu.umn.cs.ivlab.abrengine",
    "displayName": "ABR Engine",
    "version": "2024.7.0",
    "unity": "2021.3",
    "description": "Artifact-Based Rendering Engine",
    "author": { "name": "IVLab", "email": "ivlab-admin@umn.edu", "url": "https://ivlab.cs.umn.edu" },
    "documentationUrl": "https://ivlab.github.io/ABREngine-UnityPackage",
    "changelogUrl": "https://ivlab.github.io/ABREngine-UnityPackage/manual/changelog.html",
    "licensesUrl": "https://ivlab.github.io/ABREngine-UnityPackage/manual/license.html",
    "hideInEditor": "false",
    "dependencies": {
        "com.unity.textmeshpro": "2.1.6",
        "com.unity.nuget.newtonsoft-json": "3.0.2"
    },
    "samples": [
        {
            "displayName": "ABR Vis App",
            "description": "This is the main visualization app that should be used if you want to just connect to an ABR design interface. See 'Tutorials > ABR Vis App' in the documentation.",
            "path": "Samples~/ABR Vis App"
        },
        {
            "displayName": "ABR CSharp Starter",
            "description": "Contains a starter setup for creating an ABR visualization using C#. See 'Tutorials > ABR C# Visualization' in the Documentation.",
            "path": "Samples~/ABR CSharp Starter"
        },
        {
            "displayName": "Advanced Usage",
            "description": "Contains a series of scripts, states, and configurations to demonstrate advanced functionality in ABR, see 'Tutorials > Advanced Usage' in the Documentation.",
            "path": "Samples~/Advanced Usage"
        },
        {
            "displayName": "Documentation Examples",
            "description": "Examples for documentation, including GIF generator",
            "path": "Samples~/Documentation Examples"
        }
    ]
}
